class Solution(object):
    def twoSum(self, nums, target):
        size = len(nums)
        for i in range(size):
            for j in range(size):
                if (i != j):
                    if(nums[i]+nums[j] == target):
                        return [i,j]
       

        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """
        
